; ---- CHIP-8 EMU Welcome screen ---
OPTION BINARY
ALIGN OFF


HIGH		; Hires Superchip mode (128x64)

CLS		             ; clear screen
LD V8,32
LD I,SPR_LOGO_0

DRAWLOOP:
  DRW V0,V1,0
  ADD V0,16
  ADD I,V8
  SE V0,128     ; if line filled skip 
  JP DRAWLOOP   ; draw next sprite
  ADD V1,16
  LD V0,0
  SE V1,64      ; if image filled skip
  JP DRAWLOOP   ; draw next sprite



ENDLOOP:
  JP ENDLOOP

